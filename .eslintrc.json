{
    "env": {
        "node": true,
        "es2021": true
    },
    // "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        // "plugin:@typescript-eslint/recommended",
        // "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    // "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        // "@typescript-eslint",
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console":"off",
      "class-methods-use-this":"off",
      "import/first": "off",
      "no-param-reassign":"off",
      "camelcase":"off"
	   	// "import/extensions": [
	    //   "error",
      //   "ignorePackages",
	    //   {
	    //     "ts": "never"
	    //   }
	    // ]
	  }
	  // "settings": {
	  //   "import/resolver": {
	  //     "typescript": {}
	  //   }
    // }
}
